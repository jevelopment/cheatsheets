git init			# makes the folder you init into a github folder
git push			# pushes the local repo to a remote one like github.com
git pull			# pulls down the latest changes to local repo
git clone			# downloads and clones a remote repo

git config --global user.name 'My Name'		# adds name to local repo
git config --global user.email 'my@mail.com'	# adds mail to local repor

ignore (not a command) 		# create the file .gitignore in the folder. Add file names or directories in the file - plain text . to ignore folder, add: /dir to the ignorefile

git remote add origin https://github.com/jevelopment/gitcheatsheet.git	# adds remote repo
git push -u origin master	# pushes the local repo to the remote repo

git pull https://github.com/jevelopment/gitcheatsheet.git 		# downloads changes from remote repo
 
